{
  "name": "javascript-to-purescript",
  "description": "Javascript to Purescript Tutorial Series",
  "version": "2.0.0",
  "scripts": {
    "clean": "rm -rf bower_components output .psc* .pulp-cache",
    "install": "psc-package install",
    "build": "pulp --psc-package build",
    "exec": "pulp --psc-package run",
    "all": "npm run clean && npm run install && npm run build && npm run exec"
  }
}
